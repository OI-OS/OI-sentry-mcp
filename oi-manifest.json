  "parameter_extractors": {
    "find_organizations": {
      "OI-sentry-mcp::find_organizations.query": "regex:(?:query|search|find|for)[\\s:]+([^\\s]+(?:\\s+[^\\s]+)*)"
    },
    "find_projects": {
      "OI-sentry-mcp::find_projects.organizationSlug": "regex:(?:org|organization)[\\s:]+([^\\s/]+)|regex:([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)",
